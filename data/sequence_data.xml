<odoo>
    <data noupdate="1">
        <!-- Define the sequence for stock transfer requests -->
        <record id="seq_stock_transfer_request_form" model="ir.sequence">
            <field name="name">Stock Transfer Request</field>
            <field name="code">stock.transfer.request</field>
            <field name="prefix">ITR/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
